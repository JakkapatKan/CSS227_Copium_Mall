<%- include('partials/header.ejs') %>

<link rel='stylesheet' href='/stylesheets/form.css'>

<style>
   
    
</style>

<section class="py-5 text-center container" >
<form action='/register' method='POST' enctype="multipart/form-data">



    <main class="login-form">
        <div class="cotainer">
            <div class="row justify-content-center">
                <div class="col-md-8">
                    <div class="card">
                        <div class="card-header">Register</div>
                        <div class="card-body">
                            <form action="" method="">

                                <div class="form-group row row-footsies">
                                    <label for="email_address" class="col-md-4 col-form-label text-md-right">Username</label>
                                    <div class="col-md-6">
                                        <input type="text"  class="form-control" name="username" required autofocus>
                                    </div>
                                </div>
    
                                <div class="form-group row row-footsies">
                                    <label for="password" class="col-md-4 col-form-label text-md-right">Password</label>
                                    <div class="col-md-6">
                                        <input type="password" id="password" class="form-control" name="password" >
                                    </div>
                                </div>
                                
                                <div class="form-group row row-footsies">
                                    <label for="email_address" class="col-md-4 col-form-label text-md-right">First Name</label>
                                    <div class="col-md-6">
                                        <input type="text" id="email_address" class="form-control" name="firstName" >
                                    </div>
                                </div>
                                
                                <div class="form-group row row-footsies">
                                    <label for="email_address" class="col-md-4 col-form-label text-md-right">Last Name</label>
                                    <div class="col-md-6">
                                        <input type="text" id="email_address" class="form-control" name="lastName" >
                                    </div>
                                </div>
                                
                                <div class="form-group row row-footsies">
                                    <label for="email_address" class="col-md-4 col-form-label text-md-right">E-Mail</label>
                                    <div class="col-md-6">
                                        <input type="email" id="email_address" class="form-control" name="email" >
                                    </div>
                                </div>
                                
                                <div class="form-group row row-footsies">
                                    <label for="" class="col-md-4 col-form-label text-md-right">Address</label>
                                    <div class="col-md-6">
                                        <input type="text" id="email_address" class="form-control" name="address">
                                    </div>
                                </div>
                                
                                <div class="form-group row row-footsies">
                                    <label for="email_address" class="col-md-4 col-form-label text-md-right">Profile Image</label>
                                    <div class="col-md-6">
                                        <input type="file" id="email_address" class="form-control" name="profileImage" accept="image/" >
                                    </div>
                                </div>
                                
                                <!-- <div class="form-group row">
                                    <label for="email_address" class="col-md-4 col-form-label text-md-right">Admin Code</label>
                                    <div class="col-md-6">
                                        <input type="text" id="email_address" class="form-control" name="adminCode" required autofocus>
                                    </div>
                                </div> -->

                                <div class="col-md-6 offset-md-4">
                                    <button type="submit" class="btn btn-primary">
                                        Register
                                    </button>
                                    <a href="/login" class="btn btn-link">
                                        Have an account?
                                    </a>
                                </div>
                        </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
        </div>
    
    </main>





</form>
</section>

<%- include('partials/footer.ejs') %>